# Multi-resume: forking effect (resume is Clone)

effect choose(): Bool

fn choices() {
    let a = choose()
    let b = choose()
    print("{a}, {b}")
}

# Fork handler - resumes multiple times
handle choose(resume: Clone) {
    resume(true)
    resume(false)
}
choices()
# Prints: true, true
#         true, false
#         false, true
#         false, false
